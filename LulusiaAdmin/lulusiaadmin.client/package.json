{
  "name": "lulusiaadmin.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "tsc src/proxy.conf.ts --outDir dist",
    "start": "ng serve --proxy-config dist/proxy.conf.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts5": "^5.10.9",
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@ckeditor/ckeditor5-angular": "^9.1.0",
    "@coreui/angular": "^5.3.4",
    "@coreui/angular-chartjs": "^5.3.4",
    "@coreui/chartjs": "^4.0.0",
    "@coreui/coreui": "^5.2.0",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-angular": "^5.3.4",
    "@coreui/utils": "^2.0.2",
    "chart.js": "^4.4.7",
    "jest-editor-support": "*",
